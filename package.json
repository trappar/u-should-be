  "scripts": {
    "test": "jest",
    "start": "concurrently -k -p '{name}' -c 'magenta,blue' -n 'SYMFONY,WEBPACK' 'bin/console server:run' 'cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.dev.js'",
    "build": "rimraf web/dist/* && cross-env NODE_ENV=production webpack --config config/webpack.config.prod.js",
    "startProd": "npm run build && bin/console cache:clear --env=prod && bin/console server:run --env=prod"
  },
